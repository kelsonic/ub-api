{
  "name": "ub-api",
  "version": "0.0.1",
  "description": "An API that aggregates all characters, locations, and concepts from the Urantia Book",
  "main": "index.js",
  "engine": {
    "node": "16.17.0"
  },
  "scripts": {
    "convert-html-to-json": "./scripts/convert-html-to-json.sh",
    "dev": "nodemon -r tsconfig-paths/register src/server.ts",
    "migrate": "npx prisma migrate dev",
    "prettier": "prettier --check .",
    "prettier-fix": "prettier --write .",
    "start": "ts-node -r tsconfig-paths/register src/server.ts",
    "update-algolia": "yarn convert-html-to-json && yarn update-algolia-ub-node-index",
    "update-algolia-ub-node-index": "ts-node -r tsconfig-paths/register scripts/update-algolia-ub-node-index.ts"
  },
  "author": "@kelsonic",
  "license": "MIT",
  "_moduleAliases": {
    "@controllers": "dist/controllers",
    "@middleware": "dist/middleware",
    "@routes": "dist/routes",
    "@scripts": "dist/scripts",
    "@services": "dist/services",
    "@utils": "dist/utils"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "@prisma/client": "^5.5.0",
    "@types/express": "^4.17.17",
    "@types/node": "^20.5.9",
    "algoliasearch": "^4.20.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.2",
    "helmet": "^7.0.0",
    "morgan": "^1.10.0",
    "nodemon": "^3.0.1",
    "prisma": "^5.5.0",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.2.2",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.4",
    "@types/cors": "^2.8.14",
    "@types/morgan": "^1.9.5",
    "husky": "^8.0.3",
    "lint-staged": "^14.0.1",
    "prettier": "^3.0.3"
  }
}
